<div class="user-container">
  <form class="dashboard-post-form">
    {{textarea value=name placeholder="What's happening?" autofocus="autofocus"}}


    <p class="post-chars-left">{{postLength}} characters left</p>
    <div class="post-submit-container">
      <input type="submit" value="Publish" class="publish-post">
    </div>
  </form>

  <nav class="user-nav">
    <h3>Latest Posts</h3>
  </nav>

  {{partial "user/index"}}

</div>
